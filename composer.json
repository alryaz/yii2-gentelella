{
  "name": "sajphon/yii2-gentelella",
  "description": "Updated version of gentelella - free admin template based on bootstrap 3",
  "type": "yii2-extension",
  "keywords": ["yii2","extension","backend","bootsprap3","admin","gentelella","theme","asset"],
  "license": "MIT",
  "authors": [
    {
      "name": "Mario Chamuty",
      "email": "chamuty@versiontwo.sk",
      "homepage": "https://versiontwo.sk",
      "role": "Fork"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "rmrevin/yii2-fontawesome": "*",
    "bower-asset/gentelella": "*"
  },
  "autoload": {
    "psr-4": {
      "yiister\\gentelella\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
